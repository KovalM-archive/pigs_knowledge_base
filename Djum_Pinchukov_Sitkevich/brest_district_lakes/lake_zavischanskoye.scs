    [1,32 км2] (* <- lang_ru;; *);

=> nrel_depth: 
    [10,7 м] (* <- lang_ru;; *); 

=> nrel_max_width: 
    [1,02км] (* <- lang_ru;; *);    

=> nrel_lenght:
    [1,61 км] (* <- lang_ru;; *);

=> nrel_length_of_waterside:
    [4,55 км] (* <- lang_ru;; *);    

=> nrel_water_volume:
    [7,05 млн.м3] (* <- lang_ru;; *);   

