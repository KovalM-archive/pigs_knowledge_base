    [оз. Любань] (* <- lang_ru;; *);

<-concept_lake;

=> nrel_osm_query:
[( 
area["name:ru"="Любань"]["name"="Любань"]["natural"="water"];
way["name:ru"="Любань"]["name"="Любань"]["natural"="water"]; 
relation["name:ru"="Любань"]["name"="Любань"]["natural"="water"];
); 
out body; 
>; 
out skel qt;]; 

=> nrel_square: 
    [1,83 км2] (* <- lang_ru;; *);

=> nrel_depth: 
    [3,8 м] (* <- lang_ru;; *); 

=> nrel_max_width: 
    [1,4 км] (* <- lang_ru;; *);    

=> nrel_lenght:
    [2,2 км] (* <- lang_ru;; *);

=> nrel_water_intake_area:
    [143 км2] (* <- lang_ru;; *);

